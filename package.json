{
  "name": "@bigcommerce/catalyst",
  "description": "Catalyst is BigCommerce's composable front-end framework designed for building custom ecommerce storefronts.",
  "keywords": [
    "bigcommerce",
    "ecommerce",
    "storefront",
    "headless",
    "composable",
    "nextjs"
  ],
  "license": "MIT",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "dotenv -e .env.local -- turbo run dev",
    "build": "dotenv -e .env.local -- turbo run build",
    "delivery-api": "openapi --input http://localhost:22468/umbraco/swagger/delivery/swagger.json --output core/ec-core/umbracoClient --postfixServices Resource --useOptions",
    "forms-api": "openapi --input http://localhost:22468/umbraco/swagger/forms-delivery/swagger.json --output core/ec-core/formsClient --postfixServices Resource --useOptions",
    "lint": "dotenv -e .env.local -- turbo lint",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.12",
    "dotenv-cli": "^8.0.0",
    "openapi-typescript-codegen": "^0.29.0",
    "turbo": "^2.3.4",
    "typescript": "^5.7.3"
  }
}
